<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>PDX Industrial</title>
    <link rel="stylesheet" href="_compiled/css/index.css">
  </head>
  <body>

    <div class="row" id="app">
      <% projects.forEach(project => { %>
        <div class="col-xs-12 col-sm-6 col-md-4 col-lg-3 grid-item">
          <img src="_compiled/img/<%= project.img %>" />
        </div>
      <% }); %>
    </div>

    <script src="_compiled/js/app.js" />
  </body>
</html>
